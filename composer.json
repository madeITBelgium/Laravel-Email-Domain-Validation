{
  "name": "madeitbelgium/laravel-email-domain-validation",
  "version": "1.0.4",
  "description": "PHP Laravel Email domain validator",
  "keywords": ["email", "domainname"],
  "homepage": "http://www.madeit.be",
  "type": "library",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Tjebbe Lievens - Made I.T.",
      "homepage": "https://www.madeit.be",
      "email": "tjebbe.lievens@madeit.be"
    }
  ],
  "require": {
    "php": ">=5.6",
    "illuminate/support": "5.*|6.*",
    "illuminate/container": "5.*|6.*",
    "illuminate/validation": "5.*|6.*"
  },
  "require-dev": {
      "phpunit/phpunit": "5.*",
      "mockery/mockery": "0.9",
      "php-coveralls/php-coveralls": "~2.1"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
      "psr-4": {
          "MadeITBelgium\\EmailDomainValidation\\": "src/"
      }
  },
  "extra": {
       "laravel": {
           "providers": [
               "MadeITBelgium\\EmailDomainValidation\\EmailDomainServiceProvider"
           ]
       }
    }
}
